{
	"name": "cwchong/silverstripe-editorgallery",
	"type": "silverstripe-vendormodule",
	"description": "SilverStripe 4 gallery plugin in tinymce",
	"keywords": ["silverstripe", "silverstripe 4", "gallery", "htmleditor", "tinymce"],
	"license": "BSD Simplified",
	"authors": [{
		"name": "Chong Chee Wai",
		"email": "cheewai@movingmouse.com"
	}],
	"require": {
		"php": "^7.1",
		"silverstripe/framework": "^4",
    "colymba/gridfield-bulk-editing-tools": "^3"
	},
	"autoload": {
